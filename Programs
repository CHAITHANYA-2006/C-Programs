//PROGRAM 21 : Write a c program that caluclates the volume of a sphere
#include <stdio.h>
int main()
{
    float radius,volume;
    printf("Enter the radius of a sphere :");
    scanf("%f",&radius);
    volume=1.33*3.14*radius*radius*radius;
    printf("The volume of sphere is %.2f",volume);
    return 0;
}

//PROGRAM 22 : CALUCLATE GCD OF TWO NUMBERS USING FOR LOOP AND IF STATEMENT
#include<stdio.h>
int main()
{
	int i,numA,numB,GCD;
	printf("Enter number A: ");
	scanf("%d",&numA);
	printf("Enter number B: ");
	scanf("%d",&numB);
	for (i=1;i<=numA&&i<=numB;++i)
	{
		if(numA%i==0&&numB%i==0)
		GCD=i;
	}
	printf("GCD of %d and %d is %d",numA,numB,GCD); 
	return 0;
}

//PROGRAM 23 : Write a c program that converts kmph to mph (kmph=0.6213712mph)
#include <stdio.h>
int main()
{
    float kmph,mph;
    printf("Enter the speed in kmph :");
    scanf("%f",&kmph);
    mph=(0.6213712)*kmph;
    printf("The given speed in mph is %.2f",mph);
    return 0;
}

//PROGRAM 24 : Write a c program that takes hours and minutes as input and caluclates the total number of minutes
#include <stdio.h>
int main()
{
    float hours,minutes,total_time;
    printf("Enter the the time in hours :");
    scanf("%f",&hours);
    printf("Enter the the time in minutes :");
    scanf("%f",&minutes);
    total_time=(minutes+(hours)*60);
    printf("The total time in minutes is %.2f :",total_time);
    return 0;
}

//PROGRAM 25 : Write a c program to find the third angle using of triangle if two angles are given
#include <stdio.h>
int main()
{
    float first_angle,second_angle,third_angle;
    printf("Enter the first angle in degrees :");
    scanf("%f",&first_angle);
    printf("Enter the second angle in degrees :");
    scanf("%f",&second_angle);
    third_angle=180-first_angle-second_angle;
    printf("The third angle of triangle is %.2f :",third_angle);
    return 0;
}

//PROGRAM 26 : Write a c program to caluclate simple interest
#include <stdio.h>
int main()
{
    float simple_interest,principal_amount,rate_of_interest,time_period;
    printf("Enter the principal amount in rupees :");
    scanf("%f",&principal_amount);
    printf("Enter the rate of interest :");
    scanf("%f",&rate_of_interest);
    printf("Enter the time period in years :");
    scanf("%f",&time_period);
    simple_interest=(principal_amount*rate_of_interest*time_period)/100;
    printf("The simple interest by given values is %.2f :",simple_interest);
    return 0;
}

//PROGRAM 27: Write a c progaram to find the percentage of a student secured in a exam
#include <stdio.h>
#define maxmarks 600
int main() 
{
    int studentmarks;
    float percentage;
    printf("Enter the marks of a student out of 600 :");
    scanf("%d",&studentmarks);
    percentage=((float)studentmarks/maxmarks)*100;
    printf("The percentage of is %.2f:",percentage);
    return 0;
    
}

//PROGRAM 28 : Write a c program to check whether a given number is even or odd
#include <stdio.h>
int main() 
{
    int number;
    printf("Enter a positive integer :");
    scanf("%d",&number);
    if (number%2==0)
    printf("The number is even");
    else if (number%2==1)
    printf("The number is odd");
    else
    printf("Please enter a positive integer");
    return 0;
    
}

//PROGRAM 29 :Write a c program to check whether a given number is positive or negative
#include <stdio.h>
int main() 
{
    int number;
    printf("Enter an integer :");
    scanf("%d",&number);
    if (number>0)
    printf("The integer is positive");
    else if (number<0)
    printf("The integer is negative");
    else
    printf("Zero is neither positive nor negative");
    return 0;
    
}

//PROGRAM 30 : Write a c program to check whether a given year is a leap year or not
#include <stdio.h>
int main() 
{
    int year;
    printf("Enter an year:");
    scanf("%d",&year);
    if (year%4==0)
    printf("It is a leap year");
    else
    printf("it is a non leap year");
    return 0;
    
}

